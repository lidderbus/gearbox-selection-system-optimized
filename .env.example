# 应用配置
VITE_APP_TITLE=船用齿轮箱选型系统
VITE_APP_VERSION=2.0.0
VITE_APP_DESCRIPTION=智能化船舶动力解决方案

# API配置
VITE_API_URL=http://localhost:3001/api
VITE_API_TIMEOUT=15000

# 认证配置
VITE_AUTH_TOKEN_KEY=gearbox_auth_token
VITE_REFRESH_TOKEN_KEY=gearbox_refresh_token

# 功能开关
VITE_ENABLE_3D_VIEWER=true
VITE_ENABLE_AI_ASSISTANT=true
VITE_ENABLE_REAL_TIME_PRICING=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEV_TOOLS=true

# 第三方服务配置
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_MAPBOX_ACCESS_TOKEN=

# 开发模式配置
VITE_DEV_MODE=development
VITE_LOG_LEVEL=debug
VITE_MOCK_API=false

# 文件上传配置
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,application/pdf

# 缓存配置
VITE_CACHE_TTL=300000
VITE_ENABLE_CACHE=true
